.udon-market__trading-btn {
	background:  #017890;
	border: none;
	border-radius: 20px;
	box-sizing: border-box;
	color: white;
	float: right;
	height: 32px;
	outline: none;
	width: 8rem;

	&:hover {
		background: #009DC4;
		
	}
}

.redirect-to-market__btn {
	background: #017890;
    border: none;
    border-radius: 30px;
    color: white;
    font-size: 1.2rem;
    margin-top: 20px;
    padding: 10px 30px;
	
	&:hover {
		background: #009DC4;
		
	}
}
